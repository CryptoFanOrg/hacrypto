import "SHA-0-1.cry";
m <- llvm_load_module "ccsha1_ltc.bc";
let one = rewrite (cryptol_ss ()) {{ 1 : [64] }};
s <- fresh_symbolic "s" {| [5][32] |};
inp <- fresh_symbolic "inp" {| [64][8] |};
cc_md5 <- llvm_symexec m "sha1_compress_simple"
            [("s", 5), ("in", 64)]
            [("*s", s, 5), ("nblocks", one, 1), ("*in", inp, 64)]
            [("*s", 5)]
            false;
